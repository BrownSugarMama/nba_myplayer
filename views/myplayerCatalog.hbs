<h2>{{myplayers.length}} MyPlayers</h2>
{{#each myplayers as |myplayer index|}}
<h3>
  <a href="/myplayers/{{myplayer.postion }}">{{myplayer.height}} for the position of {{myplayer.archtype1}}</a>
</h3>
{{/each}}

<form action="/myplayers" method="post">
  <input type="text" position="myplayer[position]" placeholder="Position" />
  <input type="number" position="myplayer[archetype1]" placeholder="Archetype" />
  <button type="submit">Add a player</button>
</form>
